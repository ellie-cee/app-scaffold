[Unit]
    Description=uWSGI instance to serve YWM quickstart
    After=network.target

    [Service]
    User=ywm
    Group=ywm
    WorkingDirectory=/var/www/{{project_slug}}/app
    ExecStart=/usr/local/bin/pipenv run uwsgi --ini uwsgi.ini
    Restart=always

    [Install]
    WantedBy=multi-user.target
